{
  "name": "cpf-check",
  "version": "1.1.3",
  "description": "Validador de CPF Moderno, Isom√≥rfico e Super Leve.",
  "main": "dist/cpf.min.js",
  "scripts": {
    "test": "cross-env NODE_ENV=test nyc mocha src/**/*.spec.js --compilers=babel-core/register",
    "start": "npm run clean && cross-env NODE_ENV=development webpack --colors",
    "cover": "npm test && nyc report -x ./src/index.spec.js --reporter=text-lcov | coveralls",
    "build": "npm run lint && npm test && npm run clean && cross-env NODE_ENV=production webpack --colors",
    "lint": "eslint webpack.config.js && eslint src/index.js",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/flasd/cpf-check.git"
  },
  "keywords": [
    "cpf",
    "validar",
    "validate",
    "validator",
    "gerar",
    "generate",
    "generator",
    "format",
    "formatar",
    "formatter"
  ],
  "author": "Marcel de Olvieira Coelho",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/flasd/cpf-check/issues"
  },
  "homepage": "https://github.com/flasd/cpf-check#readme",
  "devDependencies": {
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-rewire": "^1.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-minify": "^0.2.0",
    "chai": "^4.1.0",
    "coveralls": "^2.13.1",
    "cross-env": "^5.0.2",
    "eslint": "^3.19.0",
    "eslint-config-airbnb-base": "^11.3.1",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "flow-bin": "^0.52.0",
    "flow-remove-types": "^1.2.1",
    "mocha": "^3.5.0",
    "nyc": "^11.1.0",
    "rewire": "^2.5.2",
    "rimraf": "^2.6.1",
    "webpack": "^3.8.1",
    "webpack-bundle-analyzer": "^2.9.1"
  },
  "nyc": {
    "include": [
      "src/index.js"
    ]
  }
}
